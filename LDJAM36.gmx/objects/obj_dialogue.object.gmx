            <string>draw_set_font(courier);
draw_set_colour(c_white);

message_end = array_length_1d(message);

draw_text(0, 100, "olar");

if (message_end &gt; 0)
{
    var charWidth = 15;
    var lineEnd = 35;
    var line = 0;
    var space = 0;
    var i = 1;
    var delay = 1;
    
    

    // Text position
    if (portrait = "none") tX = 0;
    else tX = 0;
    tY = 100;
    
   
    
    if (cutoff &lt; string_length(message[message_current]))
    {
        if (timer &gt;= delay)
        {
            cutoff++;
            timer = 0;
        }
        else timer++;
    }
    
    // Next Message
    if ( keyboard_check_pressed(vk_enter))
    {
        // If we still have some more messages, go to the next one
        if (message_current &lt; message_end-1)
        {
            message_current++;
            cutoff = 0;
        }
        else
        {
            done = true;
        }
         //show_debug_message(message_current);
    }
    else
    {
        if (message_current = 0 || message_current = 3
        || message_current = 5) &amp;&amp; keyboard_check_pressed(vk_enter)
        {
            // If we still have some more messages, go to the next one
            if (message_current &lt; message_end-1)
            {
                message_current++;
                cutoff = 0;
            }
            else
            {
                done = true;
            }
            show_debug_message(message_current);
        }
    }
    
    // Draw Text
    while(i &lt;= string_length(message[message_current]) &amp;&amp; i &lt;= cutoff)
    {
        // Check for modifier
        if(string_char_at(message[message_current],i) = "\")
        {
            modifier = real(string_char_at(message[message_current], ++i));
            ++i;
        }
    
        var length = 0;
        while(string_char_at(message[message_current],i) != " " &amp;&amp; i &lt;= string_length(message[message_current]))
        {
            i++;
            length++;
        }
        
        if (space + length &gt; lineEnd)
        {
            space = 0;
            line++;
        }
        i -= length;
        
        switch (modifier)
        {
            case 0: // Normal
            {
                draw_set_colour(c_white);
                draw_text(tX+(space*charWidth), tY+(line), string_char_at(message[message_current],i));
                break;
            }
            
            case 1: // Skip line
            {
                space = 0;
                line++;
                modifier = 0;
                break;
            }
            
            case 2: // Shaky
            {
                draw_set_colour(c_white);
                draw_text(tX+(space*charWidth) + random_range(-1,1), tY+(20*line) + random_range(-1,1), string_char_at(message[message_current],i));
                break;
            }
        }
        
        space++;
        i++;
    }
    
    // Draw Portrait
    switch (portrait)
    {
        case "none":
        {
            break;
        }
        case "anyone":
        {
            // Draw sprite portrait
            break;
        }      
    }
}
