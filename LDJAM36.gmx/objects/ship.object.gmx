        <arguments>
          <argument>
            <kind>1</kind>
            <string>/*
Otavio : movimentacao do navio
*/

show_debug_message(string(direction));

var dirFlag, opFlag;
dirFlag = 0;
opFlag = 0;

friction = 0.1;

if angle_difference(wind.windDir, direction) &lt; 45 {
    dirFlag = 1;
}

if angle_difference(wind.windDir, direction) &gt; 135 {
    opFlag = 1;
}

if (max(keyboard_check(vk_left), keyboard_check(ord("A")), 0)) direction += 1.0;

if (max(keyboard_check(vk_right), keyboard_check(ord("D")), 0)) direction -= 1.0;

if (max(keyboard_check(vk_up), keyboard_check(ord("W")), 0)) {
    if (dirFlag) {
        if (speed &lt; wind.windStr*1.0) {
            speed += 0.2;
        }        
    }
    else if (opFlag) {
        if (speed &lt; wind.windStr*0.5) {
            speed += 0.125;
        }        
    }
    else if (speed &lt; wind.windStr*0.75) {
            speed += 0.15;
    } 
    
    if (dirFlag) {
        if (speed &gt; wind.windStr*1.0) {
            speed -= 0.125;
        }        
    }
    else if (opFlag) {
        if (speed &gt; wind.windStr*0.5) {
            speed -= 0.2;
        }        
    }
    else if (speed &gt; wind.windStr*0.75) {
            speed -= 0.15;
    } 
}
    
image_angle = direction;
</string>
