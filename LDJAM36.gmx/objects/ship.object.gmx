            <string>/*
Otavio : movimentacao do navio
*/

var dirFlag, opFlag;
dirFlag = 0;
opFlag = 0;

friction = 0.1;

if angle_difference(wind.windDir, direction) &lt; 45 {
    dirFlag = 1;
}

if angle_difference(wind.windDir, direction) &gt; 135 {
    opFlag = 1;
}

if keyboard_check(vk_left) direction += 1.0;

if keyboard_check(vk_right) direction -= 1.0;

if keyboard_check(vk_up) {
    if (dirFlag) {
        if (speed &lt; wind.windStr*1.0) {
            speed += 0.2;
        }        
    }
    else if (opFlag) {
        if (speed &lt; wind.windStr*0.5) {
            speed += 0.125;
        }        
    }
    else if (speed &lt; wind.windStr*0.75) {
            speed += 0.15;
    } 
    
    if (dirFlag) {
        if (speed &gt; wind.windStr*1.0) {
            speed -= 0.125;
        }        
    }
    else if (opFlag) {
        if (speed &gt; wind.windStr*0.5) {
            speed -= 0.2;
        }        
    }
    else if (speed &gt; wind.windStr*0.75) {
            speed -= 0.15;
    } 
}
    
image_angle = direction;
</string>
